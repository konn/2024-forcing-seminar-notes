let dense-arrow = Arrow.of-bth (|
  body = NCSqArrowBody.solid;
  head = NCSqArrowHead.vee;
  tail = NCSqArrowTail.duplicate true 2 0.2 NCSqArrowTail.hook-l;
|)
let dense-arrow-flip = Arrow.of-bth (|
  body = NCSqArrowBody.solid;
  head = NCSqArrowHead.vee;
  tail = NCSqArrowTail.duplicate true 2 0.2 NCSqArrowTail.hook-r;
|)
let-math \dense-to = 
  open ArrowCommands in ${\draw!(dense-arrow)}
